<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="550.0" prefWidth="900.0" alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.it.unipr.controller.FeesController">
	<children>
		<Text fx:id="info" strokeType="OUTSIDE" text="" textAlignment="CENTER">
			<font>
				<Font name="Arial" size="12.0" />
			</font>
			<VBox.margin>
				<Insets bottom="10.0" left="0.0" right="0.0" top="0.0" />
			</VBox.margin>
		</Text>
		<TableView fx:id="feesTable" prefHeight="300.0">
			<columns>
				<TableColumn fx:id="idColumn" prefWidth="50.0" resizable="false" text="Id" />
				<TableColumn fx:id="typeColumn" prefWidth="225.0" resizable="false" text="Type" />
				<TableColumn fx:id="amountColumn" prefWidth="100.0" resizable="false" text="Amount" />
				<TableColumn fx:id="validityPeriodColumn" prefWidth="100.0" resizable="false" text="Validity Period" />
			</columns>
			<VBox.margin>
				<Insets bottom="15.0" left="30.0" right="30.0" top="0.0" />
			</VBox.margin>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</TableView>
   </children>
</VBox>